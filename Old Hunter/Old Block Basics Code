from pybricks.hubs import PrimeHub
from pybricks.parameters import Axis, Button, Color, Direction, Port
from pybricks.pupdevices import Motor
from pybricks.robotics import DriveBase
from pybricks.tools import multitask, run_task, wait

# Set up all devices.
Brain = PrimeHub(top_side=Axis.Z, front_side=Axis.X)
Right_Move = Motor(Port.E, Direction.COUNTERCLOCKWISE)
Left_Move = Motor(Port.B, Direction.CLOCKWISE)
DriveTrain = DriveBase(Right_Move, Left_Move, 62, 114)
Left_Gear = Motor(Port.A, Direction.COUNTERCLOCKWISE)
Right_Gear = Motor(Port.C, Direction.COUNTERCLOCKWISE)

# Initialize variables.
Current_program = 1
Straight_Speed = 620
Straight_Acceleration = 500
Turn_Speed = 250
Turn_Acceleration = 500
Attachment_Speed = 810
In_Leg = 0

async def Leg_1():
    await wait(1)
    await Start_Task()
    print(Current_program)

async def Leg_2():
    await wait(1)
    await Start_Task()
    print(Current_program)

async def Leg_3():
    await wait(1)
    await Start_Task()
    print(Current_program)

async def Start_Task():
    global In_Leg
    await wait(1)
    In_Leg = 1
    Brain.display.off()
    Brain.display.pixel(1, 1, 30)
    Brain.display.pixel(2, 2, 50)
    Brain.display.pixel(3, 3, 70)

async def main1():
    global Current_program, In_Leg
    Brain.imu.reset_heading(0)
    DriveTrain.settings(straight_speed=Straight_Speed)
    DriveTrain.settings(straight_acceleration=Straight_Acceleration)
    DriveTrain.settings(turn_rate=Turn_Speed)
    DriveTrain.settings(turn_acceleration=Turn_Acceleration)
    DriveTrain.straight(0)
    DriveTrain.use_gyro(True)
    while True:
        await wait(1)
        Brain.display.number(Current_program)
        Brain.light.on(Color.MAGENTA)
        if Button.RIGHT in Brain.buttons.pressed():
            Current_program = Current_program + 1
            await wait(350)
        elif Button.LEFT in Brain.buttons.pressed():
            Current_program = Current_program - 1
            await wait(350)
        elif Button.CENTER in Brain.buttons.pressed():
            if Current_program == 1:
                await Leg_1()
                await wait(300)
            elif Current_program == 2:
                await Leg_2()
                await wait(300)
            elif Current_program == 3:
                await Leg_3()
                await wait(300)
            elif Current_program == 4:
                pass
            else:
                pass
        else:
            await wait(10)
        In_Leg = 0

async def main2():
    while True:
        await wait(1)
        if In_Leg == 1:
            await wait(200)
            Brain.system.set_stop_button(Button.CENTER)
        else:
            Brain.system.set_stop_button(None)

async def main3():
    while True:
        await wait(1)
        await wait(100)
        print(Brain.imu.heading(), Left_Gear.angle(), Right_Gear.angle(), sep=", ")


async def main():
    await multitask(main1(), main2(), main3())

run_task(main())
